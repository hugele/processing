size(640,480); //画面のサイズ size(幅、高さ)


//HSBで色を指定色相、彩度、明度、透明度
colorMode(HSB,200,100,100,100);


background(0); //背景色

stroke(255,255,31); //線の色

fill(31,127,255,127); //塗り潰しの色,透明度を50%



point(200,200); //点を描画　point(X座標,Y座標)

line(80,40,600,400);  //線を描画　line(X始点,Y始点,X終点,Y数点);

rect(300,200,200,180);  //四角を描画　rect(X,Y,幅,高さ);

ellipse(450,200,200,100); //楕円を描画　円 ellipse(中心X,中心Y,幅,高さ);
